<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.login2">

    <uses-permission android:name="com.google.android.apps.photos.permission.GOOGLE_PHOTOS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".launchscreen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".DrugDateService"
            android:exported="false" />

        <activity android:name=".EditPlan" />
        <activity android:name=".EditDrug" />
        <activity android:name=".EditProfile" />
        <activity android:name=".list_planos" />
        <activity android:name=".Login">

        </activity>
        <activity android:name=".list_inventario" />
        <activity android:name=".PlanoEmpty" />
        <activity android:name=".AddPlan" />
        <activity android:name=".Profile" />
        <activity android:name=".AddMed" />
        <activity
            android:name=".inventario"
            android:exported="true" />
        <activity android:name=".register" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>